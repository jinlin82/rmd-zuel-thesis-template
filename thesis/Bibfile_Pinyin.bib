@Article{王安羽2011,
  author   = {王安羽},
  key	= {wang an yu},
  title    = {基于中国黄金期货市场数据的GARCH族模型拟合及波动率预测效果评价},
  journal  = {有色矿冶},
  year     = {2011},
  volume   = {27},
  number   = {05},
  pages    = {62--64+52},
  abstract = {本文运用GARCH族模型对我国黄金期货市场波动进行拟合情况,并运用损失函数对GARCH,TGARCH,EGARCH三个模型的波动率预测精度进行比较。实证结果表明,对称的GARCH模型是描述市场波动性给出了很好的描述,能够对投资者,特别是我国黄金生产企业在市场风险度量与预测以及投资决策时有所帮助。},
  keywords = {波动率;;GARCH族模型;;黄金期货},
}

